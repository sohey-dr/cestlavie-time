  <% n = 1 %>
  <% for first in firsts do %>
    <h3 class="mt-4"><%= n %> ： <%= first %></h3>
    <div class="col-12">
      <div class="">
        <% for band in @bands do %>
          <% if band.order == n %>
            <%= band.name %><br>
          <% end %>
