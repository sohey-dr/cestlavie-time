create_table "members", force: :cascade, comment: "サークルに所属している人たちの管理" do |t|
     t.string "name", null: false, comment: "人の名前"
     t.integer "member_type", null: false, comment: "属性(正規、OB、他サークルかなど)"
     t.string "password_digest", null: false, comment: "パスワード用"
     t.integer "debt", comment: "どれぐらいのお金を返していないか"
     t.datetime "created_at", null: false
     t.datetime "updated_at", null: false
end